{
    "name": "signifly/laravel-addresses",
    "description": "Easily manage addresses in your Laravel app",
    "homepage": "https://github.com/signifly/laravel-addresses",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.5.43|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.5.43|~5.6.0|~5.7.0|~5.8.0",
        "signifly/laravel-builder-macros": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0|~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Addresses\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Addresses\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Addresses\\AddressesServiceProvider"
            ]
        }
    }
}
